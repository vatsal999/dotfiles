syntax on
set encoding=utf8
set number
set relativenumber
set mouse=a
set hidden
set noerrorbells
set tabstop=4 softtabstop=4 
set shiftwidth=4
set expandtab
set smartindent
set nowrap 
set noswapfile
set incsearch
set nohlsearch
set scrolloff=10
set noshowmode

inoremap " ""<left>
inoremap ' ''<left>
inoremap { {}<left>
inoremap ( ()<left>

